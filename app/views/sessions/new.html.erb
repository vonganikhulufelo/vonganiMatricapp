<h2>Log in</h2>

<%= form_tag sessions_path do %>
  <div class="form-group">
    <%= label_tag :email %>
    <%= email_field_tag :email%>
  </div>

  <div class="form-group">
    <%= label_tag :passwor%>
    <%= password_field_tag :password %>
  </div>
   <%= link_to "Sign un", users_new_path %>
  <%= submit_tag "Log in",class: "btn" %>

<% end %>
